{
  "name": "@ultrathink/infra",
  "version": "1.0.0",
  "private": true,
  "description": "AWS CDK infrastructure for Ultrathink",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "deploy:dev": "cdk deploy --all --context env=dev",
    "deploy:prod": "cdk deploy --all --context env=prod --require-approval never",
    "diff": "cdk diff --all",
    "synth": "cdk synth --all",
    "destroy": "cdk destroy --all",
    "clean": "rimraf cdk.out dist"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "aws-cdk": "^2.100.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.0.0",
    "esbuild": "^0.27.2",
    "source-map-support": "^0.5.21",
    "uuid": "^13.0.0"
  }
}
